{
  "Job": {
    "Datacenters": [
      "dc1"
    ],
    "ID": "some-job",
    "Name": "some-job",
    "TaskGroups": [
      {
        "Count": 1,
        "Name": "pfg-springboot",
        "Networks": [
          {
            "DynamicPorts": [
              {
                "HostNetwork": "default",
                "Label": "http",
                "To": 8080,
                "Value": 0
              }
            ]
          }
        ],
        "Tasks": [
          {
            "Config": {
              "ports": [
                "http"
              ],
              "image": "localhost:5000/some-job:1.0.0-SNAPSHOT",
              "network_mode": "host"
            },
            "Driver": "docker",
            "env": [
              {
                "ENV_1" : "value1",
                "ENV_2" : "value2"
              }
            ],
            "Name": "some-job",
            "Resources": {
              "CPU": 500,
              "MemoryMB": 256
            }
          }
        ]
      }
    ],
    "Type": "service"
  }
}